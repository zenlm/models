{
  "name": "@zenlm/models",
  "version": "1.0.2",
  "description": "Zen model catalog â€” model specs, pricing, and families for the Zen LM model family",
  "keywords": [
    "ai",
    "llm",
    "models",
    "zen",
    "zenlm"
  ],
  "license": "MIT",
  "author": "Zen LM",
  "homepage": "https://zenlm.org",
  "repository": {
    "type": "git",
    "url": "https://github.com/zenlm/models"
  },
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./models": {
      "types": "./dist/models.d.ts",
      "import": "./dist/models.js",
      "require": "./dist/models.cjs"
    },
    "./families": {
      "types": "./dist/families.d.ts",
      "import": "./dist/families.js",
      "require": "./dist/families.cjs"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.js",
      "require": "./dist/types.cjs"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "publish:npm": "npm publish --access public"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  }
}
